<?xml version="1.0" encoding="UTF-8"?>
<Module>
<ModulePrefs title="Back link" >
        <Require feature="dynamic-height"/>   
        <Require feature="setprefs" />  
</ModulePrefs>  
<input id="reloadValue" type="hidden" name="reloadValue" value="" />
<Content type="html"><![CDATA[
<script src="https://bbvagcs.googlecode.com/svn/trunk/js/jquery-1.js" type="text/javascript"></script>
<script src="js/jquery-ui.js"></script>
<script type="text/javascript">
    jQuery(document).ready(function(){
        var d = new Date();
        d = d.getTime();
        var aux = jQuery('#reloadValue').val();
        if (aux == undefined || aux.length == 0){
            alert("aux 1.1");
            jQuery('#reloadValue').val(d);
            jQuery('body').show();
            
        }
        else{
            alert("aux 1.2");
            jQuery('#reloadValue').val('');
            window.location.reload(true);
        }
    });
</script>
<table>
<tbody>
<tr>
<td style="width:147px;height:46px">
<div style="display:inline;float:right;"><a href="#" onclick="javascript:history.go(-1);return false; style="background-image:none;border-bottom-style:none"><img border="0" src="https://sites.google.com/a/bbva.com/global-trade-finance/boton_volver.png"></a><spanborder-collapse:separate;line-height:18px;font-size:x-large">&nbsp;&nbsp;</span></div>
</td>
</tr>
</tbody>
</table>
]]></Content>
</Module>
